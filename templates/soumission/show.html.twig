{% extends 'base.html.twig' %}

{% block title %}Soumission{% endblock %}

{% block body %}
    <h1>Soumission</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ soumission.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ soumission.title }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ soumission.description }}</td>
            </tr>
            <tr>
                <th>DureeMission</th>
                <td>{{ soumission.dureeMission }}</td>
            </tr>
            <tr>
                <th>DatePublication</th>
                <td>{{ soumission.datePublication ? soumission.datePublication|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>DateSoumission</th>
                <td>{{ soumission.dateSoumission ? soumission.dateSoumission|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>DateCreation</th>
                <td>{{ soumission.dateCreation ? soumission.dateCreation|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>DateDebut</th>
                <td>{{ soumission.dateDebut ? soumission.dateDebut|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>DateFin</th>
                <td>{{ soumission.dateFin ? soumission.dateFin|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>AdresseDepot</th>
                <td>{{ soumission.adresseDepot }}</td>
            </tr>
            <tr>
                <th>IsTuneps</th>
                <td>{{ soumission.isTuneps ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Path</th>
                <td>{{ soumission.path }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('soumission_index') }}">back to list</a>

    <a href="{{ path('soumission_edit', {'id': soumission.id}) }}">edit</a>

    {{ include('soumission/_delete_form.html.twig') }}
{% endblock %}
